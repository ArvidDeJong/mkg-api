{
    "name": "darvis/mkg-api",
    "description": "Laravel package voor communicatie met het MKG ERP systeem",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arvid",
            "email": "info@darvis.nl"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Darvis\\Mkg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
